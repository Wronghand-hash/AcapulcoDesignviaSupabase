{
  "name": "AcapulcoDesignviaSupabase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "postbuild": "find <build_folder> -type f -iname *.html -exec gzip -k {} ';' ; find <build_folder> -type f -iname *.html -exec brotli --best {} ';'",
    "start": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "cross-env TEST=unit ava ./test/specs/**/*",
    "test:e2e": "cross-env TEST=e2e ava ./test/e2e/**/*"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^7.0.3",
    "@nuxtjs/svg": "^0.3.0",
    "core-js": "^3.20.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "jpeg-js": "^0.4.3",
    "nth-check": "^2.0.1",
    "nuxt": "^2.15.8",
    "nuxt-compress": "^5.0.0",
    "nuxt-imagemin": "^1.0.0",
    "nuxt-lazy-load": "^1.2.9",
    "nuxt-supabase": "^2.2.1",
    "trim-lines": "^2.0.0",
    "vue": "^2.6.14",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.3",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/eslint-parser": "^7.16.3",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/test-utils": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "ava": "^3.15.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.4.0",
    "file-loader": "^6.2.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lqip-loader": "^2.2.1",
    "node-sass": "^6.0.1",
    "nuxt-gsap-module": "^1.6.0",
    "postcss": "^8.4.6",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.3.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    "url-loader": "^4.1.1"
  }
}
